mod Examples do

  def add(a: Int, b: Int): Int = a + b

  def hello(name: String): String = do
    "Hello" + " " + name
  end

  def fib(n: Int): Int = cond do
    n < 1 => 1
    _ => fib(n - 1) + fib(n - 2)
  end

end

let addition: Int = Examples::add(1, 2)
let mut fib_result: Int = Examples::fib(23)
fib_result = fib_result + 1

print(Examples::hello("Bob"))
