# a function with no parenthesis
def greet do
    return "Hello"
end

# a function with parenthesis but no arguments
def greeting2() do
    return "Howdy"
end

# a function with one argument
def greeting3(name) do
    return "hello " + name
end

# a function with multiple arguments
def add(a, b) do
    return a + b
end

# a function in one line
def inline do return "wow" end
